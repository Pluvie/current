#!/bin/bash
echo "Compiling."
bin/compile benchmark/current -o exe/benchmark/current -O3
bin/compile benchmark/mashpoe -o exe/benchmark/mashpoe -O3
bin/compile benchmark/tidwall -o exe/benchmark/tidwall -O3
go build -o exe/benchmark/go app/benchmark/go.go

echo
echo "Running C Current."
\time -v exe/benchmark/current

echo
echo "Running C Mashpoe."
\time -v exe/benchmark/mashpoe

echo
echo "Running C Tidwall."
\time -v exe/benchmark/tidwall

echo
echo "Running Go."
\time -v exe/benchmark/go
echo
